<div id="<%= dom_id uf_tank %>">
  <p>
    <strong>Date:</strong>
    <%= uf_tank.date %>
  </p>

  <p>
    <strong>Initial temp:</strong>
    <%= uf_tank.initial_temp %>
  </p>

  <p>
    <strong>Tank number:</strong>
    <%= uf_tank.tank_number %>
  </p>

  <p>
    <strong>Ph:</strong>
    <%= uf_tank.ph %>
  </p>

  <p>
    <strong>Viscosity:</strong>
    <%= uf_tank.viscosity %>
  </p>

  <p>
    <strong>Density:</strong>
    <%= uf_tank.density %>
  </p>

  <p>
    <strong>Reaction time:</strong>
    <%= uf_tank.reaction_time %>
  </p>

</div>
